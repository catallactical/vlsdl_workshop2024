{
  "in_pcloud": [
	"vl3d_vls/data/vls/tls_sapling_trees_near_nogro.laz"
  ],
  "out_pcloud": [
	"vl3d_vls/out/Geometric_RF_on_sapling_trees/*"
  ],
  "sequential_pipeline": [
	{
		"miner": "FPSDecorated",
		"fps_decorator": {
			"num_points": "m/10",
			"fast": true,
			"num_encoding_neighbors": 1,
			"num_decoding_neighbors": 1,
			"release_encoding_neighborhoods": false,
			"threads": -1,
			"representation_report_path": null
		},
		"decorated_miner": {
			"miner": "GeometricFeatures",
			"in_pcloud": null,
			"out_pcloud": null,
			"radius": 0.5,
			"fnames": ["linearity", "planarity", "surface_variation", "verticality", "anisotropy"],
			"frenames": ["linearity_r0_5", "planarity_r0_5", "surface_variation_r0_5", "verticality_r0_5", "anisotropy_r0_5"],
			"nthreads": -1
		}
	},
	{
		"miner": "FPSDecorated",
		"fps_decorator": {
			"num_points": "m/10",
			"fast": true,
			"num_encoding_neighbors": 1,
			"num_decoding_neighbors": 1,
			"release_encoding_neighborhoods": false,
			"threads": -1,
			"representation_report_path": null
		},
		"decorated_miner": {
			"miner": "GeometricFeatures",
			"in_pcloud": null,
			"out_pcloud": null,
			"radius": 0.75,
			"fnames": ["linearity", "planarity", "surface_variation", "verticality", "anisotropy"],
			"frenames": ["linearity_r0_75", "planarity_r0_75", "surface_variation_r0_75", "verticality_r0_75", "anisotropy_r0_75"],
			"nthreads": -1
		}
	},
	{
		"miner": "FPSDecorated",
		"fps_decorator": {
			"num_points": "m/20",
			"fast": true,
			"num_encoding_neighbors": 1,
			"num_decoding_neighbors": 1,
			"release_encoding_neighborhoods": false,
			"threads": -1,
			"representation_report_path": null
		},
		"decorated_miner": {
			"miner": "GeometricFeatures",
			"in_pcloud": null,
			"out_pcloud": null,
			"radius": 1.0,
			"fnames": ["linearity", "planarity", "surface_variation", "verticality", "anisotropy"],
			"frenames": ["linearity_r1", "planarity_r1", "surface_variation_r1", "verticality_r1", "anisotropy_r1"],
			"nthreads": -1
		}
	},
	{
		"miner": "FPSDecorated",
		"fps_decorator": {
			"num_points": "m/20",
			"fast": true,
			"num_encoding_neighbors": 1,
			"num_decoding_neighbors": 1,
			"release_encoding_neighborhoods": false,
			"threads": -1,
			"representation_report_path": null
		},
		"decorated_miner": {
			"miner": "GeometricFeatures",
			"in_pcloud": null,
			"out_pcloud": null,
			"radius": 1.5,
			"fnames": ["linearity", "planarity", "surface_variation", "verticality", "anisotropy"],
			"frenames": ["linearity_r1_5", "planarity_r1_5", "surface_variation_r1_5", "verticality_r1_5", "anisotropy_r1_5"],
			"nthreads": -1
		}
	},
	{
		"miner": "FPSDecorated",
